ExecuteButton.TextSize = 32.000
ExecuteButton.MouseButton1Click:Connect(function()
	local Health = ExecuteButton.Parent:FindFirstChild("Humanoid")
	if Health == nil then
		Health.Health = Health.Health-100
	end
end)

